<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LetterSideBar">
        <attr name="letterSize" format="dimension|reference" />
    </declare-styleable>
    <declare-styleable name="ViewPagerIndicator">
        <attr name="indicatorCount" format="integer|reference" />
        <attr name="radiusSize" format="dimension|reference" />
        <!-- 枚举类型 -->
        <attr name="layout_position">
            <enum name="middle" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>

</resources>